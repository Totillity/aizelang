# import std:"io";


trait Sized {
    method length(this: Sized) -> int;

    method get_size(this: Sized) -> int {
        return this.length();
    }
}


class Stick : Sized {
    attr size: int;

    method length(this: Stick) -> int {
        return this.size;
    }

    method throw(this: Stick) -> void {
        this.size = this.size / 2;
    }
}


def main() -> int {
    var stick = Stick::new(10);
    stick.length();
    stick.get_size();
    stick.throw();
    return 0;
}