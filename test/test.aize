import std:"io";


class Person {
    attr age: int;
    attr weight: int;

    method copy(this: Person) -> Person {
        return Person::new(this.age, this.weight);
    }
}


def main() -> int {
    var person: Person = Person::new(10, 10);
    person.age = 10;
    # var clone: Person = person.copy();
    io::print_int(person.age, 1);
    return 0;
}


